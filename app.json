{
  "expo": {
    "name": "Reactify",
    "slug": "Reactify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./icon.png",
    "splash": {
      "image": "./icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesIcloudStorage":true
    },
    "android": {
      "icon":"./icon.png"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
